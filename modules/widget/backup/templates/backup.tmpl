<div class="load_from_file">
	<h4>{{writeString=wns.backup.load_from}}</h4>
	<label for="restore_file">{{writeString=wns.backup.file}}</label>
	<select name="restore_file"></select>
	<button>{{writeString=wns.backup.load_from_local_file}}</button>
</div>
<div class="dump_to_file">
	<h4>{{writeString=wns.backup.store_to}}</h4>
	<label for="file_name">{{writeString=wns.backup.path_to_export_file}}</label>
	<span class="dirname"></span><input type="text" name="file_name"/>.sql
	<label for="dumptool">{{writeString=wns.backup.path_to_mysqldump}}</label>
	<input type="text" name="dumptool"/>
	<button>{{writeString=wns.backup.backup_to_local_file}}</button>
</div>