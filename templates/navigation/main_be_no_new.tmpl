<li id="page-tree-item-{{id}}" class="navigation_level_{{level}} {{status}}">
  <a class="{{status}} level_{{level}}{{is_inactive}}" href="{{link_prefix}}{{id}}" title="{{writeParameterizedString=edit_page;title=\{\{title\}\};position=last}}">
    <button type="button" class="{{doc_status}}" title="{{doc_status}}"></button>
  {{truncate=\{\{title\}\};length=30;position=last;}}</a> {{language_warning}}
  {{identifierContext=start;name=children}}
  <ul class="main_sub_navigation">
    {{children}}
  </ul>
  {{identifierContext=end;name=children}}
</li>